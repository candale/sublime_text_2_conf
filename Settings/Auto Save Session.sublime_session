nformation for FRAME DEFAULT-FRAME\n     _Options          = \"SHARE-LOCK KEEP-EMPTY\"\n     _Query            is NOT OPENED\n*/  /* FRAME DEFAULT-FRAME */\n&ANALYZE-RESUME\n\n\n\n\n\n/* ************************  Control Triggers  ************************ */\n\n&Scoped-define SELF-NAME C-Win\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL C-Win C-Win\nON END-ERROR OF C-Win\nOR ENDKEY OF {&WINDOW-NAME} ANYWHERE DO:\n   IF THIS-PROCEDURE:PERSISTENT\n   THEN RETURN NO-APPLY.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL C-Win C-Win\nON WINDOW-CLOSE OF C-Win\nDO:\n   APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   RETURN NO-APPLY.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-ADD\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-ADD C-Win\nON CHOOSE OF BTN-ADD IN FRAME DEFAULT-FRAME /* Add */\nDO:\n   RUN Start_Create_Record.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-CAN\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-CAN C-Win\nON CHOOSE OF BTN-CAN IN FRAME DEFAULT-FRAME /* Cancel */\nDO:\n   CASE getStatus():\n   WHEN 'ADD':U\n   THEN RUN Cancel_Create_Record.\n   WHEN 'CHA':U\n   THEN RUN Cancel_Change_Record.\n   END CASE.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-CHA\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-CHA C-Win\nON CHOOSE OF BTN-CHA IN FRAME DEFAULT-FRAME /* Change */\nDO:\n   RUN Start_Change_Record.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-CHK-EXI\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-CHK-EXI C-Win\nON CHOOSE OF BTN-CHK-EXI IN FRAME DEFAULT-FRAME /* Exit */\nDO:\n   APPLY \"CLOSE\":U TO THIS-PROCEDURE.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-CHK-FLT\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-CHK-FLT C-Win\nON CHOOSE OF BTN-CHK-FLT IN FRAME DEFAULT-FRAME /* Check fault */\nDO:\n   RUN Check_Record_Fault.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-CHK-OKE\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-CHK-OKE C-Win\nON CHOOSE OF BTN-CHK-OKE IN FRAME DEFAULT-FRAME /* Check Ok */\nDO:\n   RUN Check_Record_Checked.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-DEL\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-DEL C-Win\nON CHOOSE OF BTN-DEL IN FRAME DEFAULT-FRAME /* Delete */\nDO:\n   RUN Start_Delete_Record.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-EXI\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-EXI C-Win\nON CHOOSE OF BTN-EXI IN FRAME DEFAULT-FRAME /* Exit */\nDO:\n   APPLY \"CLOSE\":U TO THIS-PROCEDURE.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME BTN-OKE\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BTN-OKE C-Win\nON CHOOSE OF BTN-OKE IN FRAME DEFAULT-FRAME /* Ok */\nDO:\n   CASE getStatus():\n   WHEN 'ADD':U\n   THEN RUN Oke_Create_Record.\n   WHEN 'CHA':U\n   THEN RUN Oke_Change_Record.\n   END CASE.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME m_Bank\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL m_Bank C-Win\nON CHOOSE OF MENU-ITEM m_Bank /* Bank */\nDO:\n   IF v-flg_bnk = TRUE THEN DO:\n      v-sub_prc_nam = \"crs000324.w\":U.\n      RUN Start_Program_Value.\n   END.\n   ELSE DO:\n      ASSIGN v-oms_err = {fna GetMessage 2414,v-cod_tal,'':U,'':U}.\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  \"WRN\":U,\n          OUTPUT v-flg_oke\n         ).\n   END.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME m_Currency\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL m_Currency C-Win\nON CHOOSE OF MENU-ITEM m_Currency /* Currency */\nDO:\n   IF v-flg_cur = TRUE THEN DO:\n      v-sub_prc_nam = \"crs000004.w\":U.\n      RUN Start_Program_Value.\n   END.\n   ELSE DO:\n      ASSIGN v-oms_err = {fna GetMessage 2414,v-cod_tal,'':U,'':U}.\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  \"WRN\":U,\n          OUTPUT v-flg_oke\n         ).\n   END.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME m_Exit\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL m_Exit C-Win\nON CHOOSE OF MENU-ITEM m_Exit /* Exit */\nDO:\n   APPLY \"CLOSE\":U TO THIS-PROCEDURE.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME w-cod_cur\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-cod_cur C-Win\nON LEAVE OF w-cod_cur IN FRAME DEFAULT-FRAME /* Currency */\nDO:\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-cod_cur.\n   RUN Get_Frame_Rel_Fields_cod_cur.\n   RUN Dis_Frame_Rel_Fields_cod_cur.\n   ASSIGN w-cod_cur = CAPS(w-cod_cur).\n   DISPLAY w-cod_cur WITH FRAME {&FRAME-NAME}.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-cod_cur C-Win\nON MOUSE-SELECT-DBLCLICK OF w-cod_cur IN FRAME DEFAULT-FRAME /* Currency */\nOR F3 OF w-cod_cur IN FRAME {&FRAME-NAME}\nDO:\n   RUN Start_Listbox_cod_cur.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME w-idt_bnk_acc\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-idt_bnk_acc C-Win\nON MOUSE-SELECT-DBLCLICK OF w-idt_bnk_acc IN FRAME DEFAULT-FRAME /* Bank account */\nOR F3 OF w-idt_bnk_acc IN FRAME {&FRAME-NAME}\nDO:\n   RUN Start_Listbox_idt_bnk_acc.\n   APPLY \"ENTRY\":U TO SELF.\n   RETURN NO-APPLY.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-idt_bnk_acc C-Win\nON RETURN OF w-idt_bnk_acc IN FRAME DEFAULT-FRAME /* Bank account */\nDO:\n   IF getStatus() <> 'INQ':U\n   THEN RETURN.\n\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-idt_bnk_acc.\n   RUN Get_Frame_Related_Fields_idt_bnk_acc.\n   IF v-cod_act = 'OKE':U\n   THEN DO:\n      ASSIGN tmp_bnk_acc_ext_wrk.bnk_acc_ext-rowid = v-rowid.\n      RUN Get_Record (INPUT 'CUR':U).\n   END.\n   ELSE DO:\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  v-typ_err,\n          OUTPUT v-flg_oke\n         ).\n      ASSIGN w-idt_bnk_acc = k-idt_bnk_acc.\n      DISPLAY w-idt_bnk_acc WITH FRAME {&FRAME-NAME}.\n   END.\n   APPLY \"ENTRY\":U TO SELF.\n   RETURN NO-APPLY.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME w-num_bnk\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-num_bnk C-Win\nON LEAVE OF w-num_bnk IN FRAME DEFAULT-FRAME /* Bank */\nDO:\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-num_bnk.\n   RUN Get_Frame_Rel_Fields_num_bnk.\n   RUN Dis_Frame_Rel_Fields_num_bnk.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-num_bnk C-Win\nON MOUSE-SELECT-DBLCLICK OF w-num_bnk IN FRAME DEFAULT-FRAME /* Bank */\nOR F3 OF w-num_bnk IN FRAME {&FRAME-NAME}\nDO:\n   RUN Start_Listbox_num_bnk.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&Scoped-define SELF-NAME w-typ_acc\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-typ_acc C-Win\nON LEAVE OF w-typ_acc IN FRAME DEFAULT-FRAME /* Account type */\nDO:\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-typ_acc.\n   RUN Get_Frame_Rel_Fields_typ_acc.\n   RUN Dis_Frame_Rel_Fields_typ_acc.\n   ASSIGN w-typ_acc = CAPS(w-typ_acc).\n   DISPLAY w-typ_acc WITH FRAME {&FRAME-NAME}.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL w-typ_acc C-Win\nON MOUSE-SELECT-DBLCLICK OF w-typ_acc IN FRAME DEFAULT-FRAME /* Account type */\nOR F3 OF w-typ_acc IN FRAME {&FRAME-NAME}\nDO:\n   RUN Start_Listbox_typ_acc.\nEND.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n&UNDEFINE SELF-NAME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CUSTOM _MAIN-BLOCK C-Win\n\n\n{std_fun_mai.i}\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n\n/* **********************  Internal Procedures  *********************** */\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Bepaal_Autorisatie_Jump C-Win\nPROCEDURE Bepaal_Autorisatie_Jump :\n/*------------------------------------------------------------------------------\n  Purpose : Bepaal Autorisatie Jump\n------------------------------------------------------------------------------*/\n\n   /***************************************************************************/\n   /* Autorisatie t.b.v. functie Bank                                         */\n   /***************************************************************************/\n   RUN d01_alg_aut.p\n      (\n       INPUT  \"CRS\":U,\n       INPUT  mtg-idt_usr,\n       INPUT  \"crs000324.w\":U,\n       INPUT  \"Inq\":U,\n       OUTPUT v-cod_err\n      ).\n   IF v-cod_err = 0 THEN v-flg_bnk = TRUE.\n\n   /***************************************************************************/\n   /* Autorisatie t.b.v. functie Currency                                     */\n   /***************************************************************************/\n   RUN d01_alg_aut.p\n      (\n       INPUT  \"CRS\":U,\n       INPUT  mtg-idt_usr,\n       INPUT  \"crs000004.w\":U,\n       INPUT  \"Inq\":U,\n       OUTPUT v-cod_err\n      ).\n   IF v-cod_err = 0 THEN v-flg_cur = TRUE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Cancel_Change_Record C-Win\nPROCEDURE Cancel_Change_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Cancel Change Record\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Cancel_Change_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   setStatus('INQ':U).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U      /* Record gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Enable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Enable_Browse_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Menu_Item.\n      RUN Enable_Button_Exit.\n      RUN Display_Frame.\n      RUN Fill_Parameters.\n      RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n   END. /* OKE */\n\n   WHEN 'FIL_BUF':U  /* Record niet gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Enable_Menu_Item.\n      RUN Enable_Button_Exit.\n      RUN Disable_Buttons_Wij_Ver.\n      RUN Display_Frame.\n   END. /* FIL_BUF */\n\n   END CASE.\n\n   /***************************************************************************/\n   /* Functie automatisch afsluiten indien deze in 'CHA' mode is gestart.     */\n   /***************************************************************************/\n   IF p-typ_fun = 'CHA':U THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Cancel_Create_Record C-Win\nPROCEDURE Cancel_Create_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Cancel Create Record\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Cancel_Create_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   setStatus('INQ':U).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U      /* Record gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Enable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Enable_Browse_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Button_Exit.\n      RUN Enable_Menu_Item.\n      RUN Display_Frame.\n      RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n   END. /* OKE */\n\n   WHEN 'FIL_BUF':U  /* Record niet gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Enable_Button_Exit.\n      RUN Enable_Menu_Item.\n      RUN Disable_Buttons_Wij_Ver.\n      RUN Display_Frame.\n   END. /* FIL_BUF */\n\n   END CASE.\n\n   /***************************************************************************/\n   /* Functie automatisch afsluiten indien deze in 'ADD' mode is gestart.     */\n   /***************************************************************************/\n   IF p-typ_fun = 'ADD':U THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Check_Record_Checked C-Win\nPROCEDURE Check_Record_Checked :\n/*------------------------------------------------------------------------------\n  Purpose : Check Record Checked\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Check_Record_Checked':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n   WHEN 'OKE':U      /* Record status Checked\" */\n   THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END. /* OKE */\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Check_Record_Fault C-Win\nPROCEDURE Check_Record_Fault :\n/*------------------------------------------------------------------------------\n  Purpose : Check Record Fault\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Check_Record_Fault':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n   WHEN 'OKE':U      /* Record status \"Fault\" */\n   THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END. /* OKE */\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Create_Temp_Table_Buffers C-Win\nPROCEDURE Create_Temp_Table_Buffers :\n/*------------------------------------------------------------------------------\n  Purpose : Create Temp Table Buffers\n------------------------------------------------------------------------------*/\n\n   CREATE tmp_bnk_acc_ext_wrk.\n   ASSIGN tmp_bnk_acc_ext_wrk.idt_bnk_acc = p-idt_bnk_acc.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Disable_Status_Field C-Win\nPROCEDURE Disable_Status_Field :\n/*------------------------------------------------------------------------------\n  Purpose : Disable Status Field\n------------------------------------------------------------------------------*/\n\n   DISABLE w-sta_bnk_acc WITH FRAME {&FRAME-NAME}.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Disable_UI C-Win  _DEFAULT-DISABLE\nPROCEDURE Disable_UI :\n/*------------------------------------------------------------------------------\n  Purpose:     DISABLE the User Interface\n  Parameters:  <none>\n  Notes:       Here we clean-up the user-interface by deleting\n               dynamic widgets we have created and/or hide\n               frames.  This procedure is usually called when\n               we are ready to \"clean-up\" after running.\n------------------------------------------------------------------------------*/\n  /* Delete the WINDOW we created */\n  IF SESSION:DISPLAY-TYPE = \"GUI\":U AND VALID-HANDLE(C-Win)\n  THEN DELETE WIDGET C-Win.\n  IF THIS-PROCEDURE:PERSISTENT THEN DELETE PROCEDURE THIS-PROCEDURE.\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Display_Frame_Related_Fields C-Win\nPROCEDURE Display_Frame_Related_Fields :\n/*------------------------------------------------------------------------------\n  Purpose : Display Frame Related Fields\n------------------------------------------------------------------------------*/\n\n   RUN Dis_Frame_Rel_Fields_typ_acc.\n   RUN Dis_Frame_Rel_Fields_num_bnk.\n   RUN Dis_Frame_Rel_Fields_cod_cur.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Dis_Frame_Rel_Fields_cod_cur C-Win\nPROCEDURE Dis_Frame_Rel_Fields_cod_cur :\n/*------------------------------------------------------------------------------\n  Purpose : Display Related Frame Fields of w-cod_cur\n------------------------------------------------------------------------------*/\n\n   DO WITH FRAME {&FRAME-NAME} :\n      DISPLAY w-cod_cur w-dsc_cur.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Dis_Frame_Rel_Fields_num_bnk C-Win\nPROCEDURE Dis_Frame_Rel_Fields_num_bnk :\n/*------------------------------------------------------------------------------\n  Purpose : Display Related Frame Fields of w-num_bnk\n------------------------------------------------------------------------------*/\n\n   RUN Set_Bnk_Acc_Num_Fmt.\n\n   DO WITH FRAME {&FRAME-NAME} :\n      DISPLAY w-num_bnk w-nam_bnk w-adr_bnk w-wpl_bnk w-lnd_bnk.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Dis_Frame_Rel_Fields_typ_acc C-Win\nPROCEDURE Dis_Frame_Rel_Fields_typ_acc :\n/*------------------------------------------------------------------------------\n  Purpose : Display Related Frame Fields of w-typ_acc\n------------------------------------------------------------------------------*/\n\n   DO WITH FRAME {&FRAME-NAME} :\n      DISPLAY w-typ_acc w-dsc_acc.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Enable_Check_Buttons C-Win\nPROCEDURE Enable_Check_Buttons :\n/*------------------------------------------------------------------------------\n  Purpose : Enable Check Butons\n------------------------------------------------------------------------------*/\n\n   DO WITH FRAME {&FRAME-NAME} :\n      DISPLAY BTN-CHK-EXI BTN-CHK-OKE BTN-CHK-FLT.\n      ENABLE BTN-CHK-EXI.\n\n      CASE w-sta_bnk_acc :\n      WHEN 'Registered':U\n      THEN DO:\n         IF tmp_bnk_acc_ext_wrk.idt_emp_reg <> mtg-idt_usr\n         THEN ENABLE BTN-CHK-OKE BTN-CHK-FLT.\n      END.\n\n      WHEN 'Corrected':U\n      THEN DO:\n         IF tmp_bnk_acc_ext_wrk.idt_emp_cor <> mtg-idt_usr\n         THEN ENABLE BTN-CHK-OKE BTN-CHK-FLT.\n      END.\n\n      WHEN 'Fault':U\n      THEN DO:\n      END.\n\n      WHEN 'Checked':U\n      THEN DO:\n      END.\n\n      END CASE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Enable_UI C-Win  _DEFAULT-ENABLE\nPROCEDURE Enable_UI :\n/*------------------------------------------------------------------------------\n  Purpose:     ENABLE the User Interface\n  Parameters:  <none>\n  Notes:       Here we display/view/enable the widgets in the\n               user-interface.  In addition, OPEN all queries\n               associated with each FRAME and BROWSE.\n               These statements here are based on the \"Other\n               Settings\" section of the widget Property Sheets.\n------------------------------------------------------------------------------*/\n  DISPLAY w-idt_bnk_acc w-sta_bnk_acc w-typ_acc w-dsc_acc w-num_bnk w-nam_bnk\n          w-adr_bnk w-wpl_bnk w-lnd_bnk w-cod_cur w-dsc_cur w-num_ctr_bnk_acc\n          w-nam_ctr_bnk_acc w-wpl_bnk_acc w-win_tit w-txt_fun w-txt_brw\n      WITH FRAME DEFAULT-FRAME IN WINDOW C-Win.\n  ENABLE RECT-18 RECT-19 RECT-20 RECT-21 w-idt_bnk_acc w-cod_cur BTN-JMP\n         BTN-ADD BTN-CHA BTN-DEL BTN-FST BTN-PRV BTN-NXT BTN-LST BTN-EXI\n         BTN-OKE BTN-CAN BTN-HLP\n      WITH FRAME DEFAULT-FRAME IN WINDOW C-Win.\n  {&OPEN-BROWSERS-IN-QUERY-DEFAULT-FRAME}\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Fill_Frame_Fields C-Win\nPROCEDURE Fill_Frame_Fields :\n/*------------------------------------------------------------------------------\n  Purpose : Fill Frame Fields\n------------------------------------------------------------------------------*/\n\n   ASSIGN w-typ_acc         = tmp_bnk_acc_ext_wrk.typ_acc\n          w-num_bnk         = tmp_bnk_acc_ext_wrk.num_bnk\n          w-cod_cur         = tmp_bnk_acc_ext_wrk.cod_cur\n          w-num_ctr_bnk_acc = tmp_bnk_acc_ext_wrk.num_ctr_bnk_acc\n          w-nam_ctr_bnk_acc = tmp_bnk_acc_ext_wrk.nam_ctr_bnk_acc\n          w-wpl_bnk_acc     = tmp_bnk_acc_ext_wrk.wpl_bnk_acc\n          w-sta_bnk_acc     = tmp_bnk_acc_ext_wrk.sta_bnk_acc\n          .\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Fill_Frame_Keys C-Win\nPROCEDURE Fill_Frame_Keys :\n/*------------------------------------------------------------------------------\n  Purpose : Fill Frame Keys\n------------------------------------------------------------------------------*/\n\n   ASSIGN w-idt_bnk_acc = tmp_bnk_acc_ext_wrk.idt_bnk_acc\n          k-idt_bnk_acc = tmp_bnk_acc_ext_wrk.idt_bnk_acc.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Fill_Frame_Related_Fields C-Win\nPROCEDURE Fill_Frame_Related_Fields :\n/*------------------------------------------------------------------------------\n  Purpose : Fill Frame Related Fields\n------------------------------------------------------------------------------*/\n\n   RUN Get_Frame_Rel_Fields_typ_acc.\n   RUN Get_Frame_Rel_Fields_num_bnk.\n   RUN Get_Frame_Rel_Fields_cod_cur.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Fill_Parameters C-Win\nPROCEDURE Fill_Parameters :\n/*------------------------------------------------------------------------------\n  Purpose : Fill Parameters\n------------------------------------------------------------------------------*/\n\n   ASSIGN p-idt_bnk_acc = w-idt_bnk_acc.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Get_Frame_Related_Fields_idt_bnk_acc C-Win\nPROCEDURE Get_Frame_Related_Fields_idt_bnk_acc :\n/*------------------------------------------------------------------------------\n  Purpose : Get Frame Related Fields of w-idt_bnk_acc\n------------------------------------------------------------------------------*/\n\n   RUN get_bnk_acc_ext_api.p ON ghAppServer\n      (\n       INPUT  w-idt_bnk_acc,\n       OUTPUT w-num_ctr_bnk_acc,\n       OUTPUT v-rowid,\n       OUTPUT v-cod_act,\n       OUTPUT v-typ_err,\n       OUTPUT v-oms_err\n      ).\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Get_Frame_Rel_Fields_cod_cur C-Win\nPROCEDURE Get_Frame_Rel_Fields_cod_cur :\n/*------------------------------------------------------------------------------\n  Purpose : Get Related Frame Fields of w-cod_cur\n------------------------------------------------------------------------------*/\n\n   RUN get_cur_api.p ON ghAppServer\n      (\n       INPUT  w-cod_cur,\n       OUTPUT w-dsc_cur,\n       OUTPUT v-cod_act,\n       OUTPUT v-typ_err,\n       OUTPUT v-oms_err\n      ).\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Get_Frame_Rel_Fields_num_bnk C-Win\nPROCEDURE Get_Frame_Rel_Fields_num_bnk :\n/*------------------------------------------------------------------------------\n  Purpose : Get Related Frame Fields of w-num_bnk\n------------------------------------------------------------------------------*/\n\n   EMPTY TEMP-TABLE tmp_bnk_wrk.\n   RUN bnk_get_buf_api.p ON ghAppServer\n      (\n       INPUT        w-num_bnk,\n       OUTPUT TABLE tmp_bnk_wrk,\n       OUTPUT       v-cod_act,\n       OUTPUT       v-typ_err,\n       OUTPUT       v-oms_err\n      ).\n\n   FIND FIRST tmp_bnk_wrk NO-LOCK NO-ERROR.\n   IF AVAILABLE tmp_bnk_wrk\n   THEN DO:\n      ASSIGN w-nam_bnk = tmp_bnk_wrk.nam_bnk\n             w-adr_bnk = tmp_bnk_wrk.adr_bnk\n             w-wpl_bnk = tmp_bnk_wrk.wpl_bnk.\n\n      /************************************************************************/\n      /* Ophalen landnaam van de Bank                                         */\n      /************************************************************************/\n      RUN lnd_get_api.p ON ghAppServer\n         (\n          INPUT  tmp_bnk_wrk.cod_lnd,\n          OUTPUT w-lnd_bnk,\n          OUTPUT v-rowid,\n          OUTPUT v-cod_act,\n          OUTPUT v-typ_err,\n          OUTPUT v-oms_err\n         ).\n   END.\n   ELSE DO:\n      ASSIGN w-nam_bnk = \"\":U\n             w-adr_bnk = \"\":U\n             w-wpl_bnk = \"\":U\n             w-lnd_bnk = \"\":U.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Get_Frame_Rel_Fields_typ_acc C-Win\nPROCEDURE Get_Frame_Rel_Fields_typ_acc :\n/*------------------------------------------------------------------------------\n  Purpose : Get Related Frame Fields of w-typ_acc\n------------------------------------------------------------------------------*/\n\n   RUN typ_acc_get_api.p ON ghAppServer\n      (\n       INPUT  w-typ_acc,\n       OUTPUT w-dsc_acc,\n       OUTPUT v-rowid,\n       OUTPUT v-cod_act,\n       OUTPUT v-typ_err,\n       OUTPUT v-oms_err\n      ).\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Get_Temp_Table_Buffers C-Win\nPROCEDURE Get_Temp_Table_Buffers :\n/*------------------------------------------------------------------------------\n  Purpose : Get_Temp_Table_Buffers\n------------------------------------------------------------------------------*/\n\n   FIND FIRST tmp_bnk_acc_ext_wrk NO-ERROR.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Hide_Non_Check_Buttons C-Win\nPROCEDURE Hide_Non_Check_Buttons :\n/*------------------------------------------------------------------------------\n  Purpose : Hide Non Check Buttons\n------------------------------------------------------------------------------*/\n\n   DO WITH FRAME {&FRAME-NAME} :\n      ASSIGN BTN-ADD:HIDDEN = TRUE\n             BTN-CHA:HIDDEN = TRUE\n             BTN-DEL:HIDDEN = TRUE\n             BTN-EXI:HIDDEN = TRUE\n             BTN-OKE:HIDDEN = TRUE\n             BTN-CAN:HIDDEN = TRUE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Init_Program C-Win\nPROCEDURE Init_Program :\n/*------------------------------------------------------------------------------\n  Purpose : Init Program\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Init_Startup.\n   RUN Load_Mouse_Menu.\n   RUN Bepaal_Authorisatie.\n   RUN Bepaal_Autorisatie_Jump.\n   RUN Create_Temp_Table_Buffers.\n   RUN Start_AppServer_Procedure (INPUT 'Init_Program':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   setStatus('INQ':U).\n\n   CASE v-cod_act:\n   WHEN 'OKE':U      /* Record gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Enable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Enable_Browse_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Button_Exit.\n      RUN Display_Frame.\n      RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n   END. /* OKE */\n   WHEN 'FIL_BUF':U  /* Record niet gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Buttons_Wij_Ver.\n      RUN Display_Frame.\n   END. /* FIL_BUF */\n   END CASE.\n\n   RUN Finish_Startup.\n\n   /***************************************************************************/\n   /* Afhanddelen van het functie type nadat alle initialisaties hebben       */\n   /* plaatsgevonden.                                                         */\n   /***************************************************************************/\n   CASE p-typ_fun :\n   WHEN 'ADD':U\n   THEN DO:\n      RUN Start_Create_Record.\n   END.\n   WHEN 'CHA':U\n   THEN DO:\n      IF v-cod_act = 'OKE':U\n      THEN DO:\n         RUN Start_Change_Record.\n      END.\n   END.\n   WHEN 'CHK':U\n   THEN DO:\n      /************************************************************************/\n      /* Indien er geen record wordt gevonden, dan kan er niet gecheck worden */\n      /* Er wordt een melding gegeven en het scherm wordt afgesloten.         */\n      /************************************************************************/\n      IF v-cod_act = 'OKE':U\n      THEN DO:\n         RUN Start_Check_Record.\n      END.\n   END.\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Load_Mouse_Pointers C-Win\nPROCEDURE Load_Mouse_Pointers :\n/*------------------------------------------------------------------------------\n  Purpose : Load Mouse Pointers\n------------------------------------------------------------------------------*/\n\n   DO WITH FRAME {&FRAME-NAME} :\n      IF getStatus() = 'INQ':U\n      THEN DO:\n         w-idt_bnk_acc:LOAD-MOUSE-POINTER('listbox.cur':U).\n         w-typ_acc:LOAD-MOUSE-POINTER('IBEAM':U).\n         w-num_bnk:LOAD-MOUSE-POINTER('IBEAM':U).\n         w-cod_cur:LOAD-MOUSE-POINTER('IBEAM':U).\n      END.\n      ELSE DO:\n         w-idt_bnk_acc:LOAD-MOUSE-POINTER('IBEAM':U).\n         w-typ_acc:LOAD-MOUSE-POINTER('listbox.cur':U).\n         w-num_bnk:LOAD-MOUSE-POINTER('listbox.cur':U).\n         w-cod_cur:LOAD-MOUSE-POINTER('listbox.cur':U).\n      END.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Oke_Change_Record C-Win\nPROCEDURE Oke_Change_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Oke Change Record\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Assign_Frame_Fields.\n   RUN Update_Temp_Table_Buffers.\n   RUN Start_AppServer_Procedure (INPUT 'Oke_Change_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U      /* Record gewijzigd */\n   THEN DO:\n      setStatus('INQ':U).\n      RUN Fill_Frame.\n      RUN Enable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Enable_Browse_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Menu_Item.\n      RUN Enable_Button_Exit.\n      RUN Display_Frame.\n      RUN Fill_Parameters.\n      RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n   END. /* OKE */\n\n   WHEN 'CHK_FLD':U  /* Veld controle */\n   THEN RUN Apply_Entry_To_Field (INPUT v-idt_fld).\n\n   END CASE.\n\n   /***************************************************************************/\n   /* Functie automatisch afsluiten indien deze in 'CHA' mode is gestart.     */\n   /***************************************************************************/\n   IF p-typ_fun = 'CHA':U THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Oke_Create_Record C-Win\nPROCEDURE Oke_Create_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Oke Create Record\n------------------------------------------------------------------------------*/\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Assign_Frame_Keys.\n   RUN Assign_Frame_Fields.\n   RUN Update_Temp_Table_Buffers.\n   RUN Start_AppServer_Procedure (INPUT 'Oke_Create_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n\n      WHEN 'OKE':U      /* Record toegevoegd */\n      THEN DO:\n         setStatus('INQ':U).\n         RUN Fill_Frame.\n         RUN Enable_Frame_Keys.\n         RUN Disable_Frame_Fields.\n         RUN Enable_Browse_Buttons.\n         RUN Enable_Methode_Buttons.\n         RUN Disable_Transaction_Buttons.\n         RUN Enable_Menu_Item.\n         RUN Enable_Button_Exit.\n         RUN Display_Frame.\n         RUN Fill_Parameters.\n         RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n      END. /* OKE */\n\n      WHEN 'CHK_FLD':U  /* Veld controle */\n      THEN RUN Apply_Entry_To_Field (INPUT v-idt_fld).\n\n   END CASE.\n\n   /***************************************************************************/\n   /* Functie automatisch afsluiten indien deze in 'ADD' mode is gestart.     */\n   /***************************************************************************/\n   IF p-typ_fun = 'ADD':U THEN DO:\n      APPLY \"CLOSE\":U TO THIS-PROCEDURE.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_AppServer_Procedure C-Win\nPROCEDURE Start_AppServer_Procedure :\n/*------------------------------------------------------------------------------\n  Purpose : Start AppServer Procedure\n------------------------------------------------------------------------------*/\n\n   DEFINE INPUT PARAMETER p-nam_prc AS CHARACTER FORMAT \"X(50)\":U NO-UNDO.\n\n   RUN bnk_acc_ext_api.p ON ghAppServer\n      (\n       INPUT              p-nam_prc,\n       INPUT-OUTPUT TABLE tmp_bnk_acc_ext_wrk,\n       OUTPUT             v-cod_act,\n       OUTPUT             v-typ_err,\n       OUTPUT             v-oms_err,\n       OUTPUT             v-idt_fld\n      ).\n\n   RUN Get_Temp_Table_Buffers.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Change_Record C-Win\nPROCEDURE Start_Change_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Start Change Record\n------------------------------------------------------------------------------*/\n\n   DEFINE VARIABLE v-typ_fun  AS CHARACTER   NO-UNDO.\n\n   IF NOT v-flg_cha\n   THEN DO:\n      ASSIGN\n         v-typ_fun = {fna getMessage 915,v-cod_tal,'',''}  /* change */\n         v-oms_err = {fna getMessage 913,v-cod_tal,'',\"v-typ_fun + CHR(3) + v-nam_tbl\"}\n         .\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  'INF':U,\n          OUTPUT v-flg_oke\n         ).\n      RETURN.\n   END.\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Start_Change_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U      /* Wijzigen kan beginnen */\n   THEN DO:\n      setStatus('CHA':U).\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Enable_Frame_Fields.\n      RUN Disable_Status_Field.\n      RUN Disable_Methode_Buttons.\n      RUN Disable_Button_Exit.\n      RUN Enable_Transaction_Buttons.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Menu_Item.\n      RUN Display_Frame.\n      RUN Apply_Entry_To_Field (INPUT 'typ_acc':U).\n   END. /* OKE */\n\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Check_Record C-Win\nPROCEDURE Start_Check_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Start Check Record\n------------------------------------------------------------------------------*/\n\n   DEFINE VARIABLE v-typ_fun  AS CHARACTER   NO-UNDO.\n\n   IF NOT v-flg_cha\n   THEN DO:\n      ASSIGN\n         v-typ_fun = {fna getMessage 947,v-cod_tal,'',''}  /* check */\n         v-oms_err = {fna getMessage 913,v-cod_tal,'',\"v-typ_fun + CHR(3) + v-nam_tbl\"}\n         .\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  'INF':U,\n          OUTPUT v-flg_oke\n         ).\n      RETURN.\n   END.\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Start_Check_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n   WHEN 'OKE':U      /* Checken kan beginnen */\n   THEN DO:\n      setStatus('CHK':U).\n      RUN Disable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Disable_Browse_Buttons.\n      RUN Hide_Non_Check_Buttons.\n      RUN Enable_Check_Buttons.\n   END. /* OKE */\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Create_Record C-Win\nPROCEDURE Start_Create_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Start Create Record\n------------------------------------------------------------------------------*/\n\n   DEFINE VARIABLE v-typ_fun  AS CHARACTER   NO-UNDO.\n\n   IF NOT v-flg_add\n   THEN DO:\n      ASSIGN\n         v-typ_fun = {fna getMessage 914,v-cod_tal,'',''}  /* add */\n         v-oms_err = {fna getMessage 913,v-cod_tal,'',\"v-typ_fun + CHR(3) + v-nam_tbl\"}\n         .\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  'INF':U,\n          OUTPUT v-flg_oke\n         ).\n      RETURN.\n   END.\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Start_Create_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U      /* Toevoegen kan beginnen */\n   THEN DO:\n      setStatus(\"ADD\":u).\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Enable_Frame_Fields.\n      RUN Disable_Status_Field.\n      RUN Disable_Methode_Buttons.\n      RUN Disable_Button_Exit.\n      RUN Enable_Transaction_Buttons.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Menu_Item.\n      RUN Display_Frame.\n      RUN Apply_Entry_To_Field (INPUT 'typ_acc':U).\n   END. /* OKE */\n\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Delete_Record C-Win\nPROCEDURE Start_Delete_Record :\n/*------------------------------------------------------------------------------\n  Purpose : Start Delete Record\n------------------------------------------------------------------------------*/\n\n   DEFINE VARIABLE v-typ_fun  AS CHARACTER   NO-UNDO.\n\n   IF NOT v-flg_del\n   THEN DO:\n      ASSIGN\n         v-typ_fun = {fna getMessage 916,v-cod_tal,'',''}  /* delete */\n         v-oms_err = {fna getMessage 913,v-cod_tal,'',\"v-typ_fun + CHR(3) + v-nam_tbl\"}\n         .\n      RUN Display_Message\n         (\n          INPUT  v-oms_err,\n          INPUT  'INF':U,\n          OUTPUT v-flg_oke\n         ).\n      RETURN.\n   END.\n\n   ASSIGN\n      v-typ_fun = {fna getMessage 916,v-cod_tal,'',''}  /* delete */\n      v-oms_err = {fna getMessage 921,v-cod_tal,'',\"v-typ_fun + CHR(3) + v-nam_tbl\"}\n      .\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  'QUE':U,\n       OUTPUT v-flg_oke\n      ).\n   IF v-flg_oke = FALSE\n   THEN RETURN.\n\n   SESSION:SET-WAIT-STATE(\"GENERAL\":U).\n   RUN Start_AppServer_Procedure (INPUT 'Oke_Delete_Record':U).\n   SESSION:SET-WAIT-STATE(\"\":U).\n\n   RUN Display_Message\n      (\n       INPUT  v-oms_err,\n       INPUT  v-typ_err,\n       OUTPUT v-flg_oke\n      ).\n\n   CASE v-cod_act:\n\n   WHEN 'OKE':U         /* Record verwijderd en nieuwe record gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Display_Frame.\n      RUN Apply_Entry_To_Field (INPUT 'idt_bnk_acc':U).\n   END. /* OKE */\n\n   WHEN 'FIL_BUF':U     /* Record verwijderd maar geen nieuwe record gevonden */\n   THEN DO:\n      RUN Fill_Frame.\n      RUN Disable_Frame_Keys.\n      RUN Disable_Frame_Fields.\n      RUN Disable_Browse_Buttons.\n      RUN Disable_Transaction_Buttons.\n      RUN Enable_Methode_Buttons.\n      RUN Disable_Buttons_Wij_Ver.\n      RUN Enable_Button_Exit.\n      RUN Enable_Menu_Item.\n      RUN Display_Frame.\n   END. /* FIL_BUF */\n\n   WHEN 'BUF_CHA':U     /* Record gewijzigd door een andere gebruiker */\n   THEN RUN Get_Record (INPUT 'CUR':U).\n\n   WHEN 'BUF_DEL':U     /* Record verwijderd door een andere gebruiker */\n   THEN RUN Get_Record (INPUT 'FST':U).\n\n   WHEN 'REF_INT':U OR  /* Record mag niet verwijderd worden */\n   WHEN 'BUF_LCK':U\n   THEN DO:\n   END.\n\n   END CASE.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Listbox_cod_cur C-Win\nPROCEDURE Start_Listbox_cod_cur :\n/*------------------------------------------------------------------------------\n  Purpose : Start Listbox op veld w-cod_cur\n------------------------------------------------------------------------------*/\n\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-cod_cur.\n   RUN crs900005.w\n      (\n       INPUT-OUTPUT w-cod_cur,\n       OUTPUT       w-dsc_cur,\n       OUTPUT       v-cod_can\n      ).\n\n   IF v-cod_can = FALSE\n   THEN DO:\n      RUN Get_Frame_Rel_Fields_cod_cur.\n      RUN Dis_Frame_Rel_Fields_cod_cur.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Listbox_idt_bnk_acc C-Win\nPROCEDURE Start_Listbox_idt_bnk_acc :\n/*------------------------------------------------------------------------------\n  Purpose : Start_Listbox_idt_bnk_acc\n------------------------------------------------------------------------------*/\n\n   IF getStatus() = 'INQ':U\n   THEN DO:\n      ASSIGN v-idt_bnk_acc = 0.\n      RUN lbx_bnk_acc_ext.w\n         (\n          INPUT-OUTPUT v-idt_bnk_acc,\n          OUTPUT       w-num_ctr_bnk_acc,\n          OUTPUT       v-rowid,\n          OUTPUT       v-cod_can\n         ).\n      IF v-cod_can = TRUE\n      THEN RETURN.\n\n      ASSIGN tmp_bnk_acc_ext_wrk.bnk_acc_ext-rowid = v-rowid.\n      RUN Get_Record (INPUT 'CUR':U).\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Listbox_num_bnk C-Win\nPROCEDURE Start_Listbox_num_bnk :\n/*------------------------------------------------------------------------------\n  Purpose : Start Listbox op veld w-num_bnk\n------------------------------------------------------------------------------*/\n\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-num_bnk.\n   RUN crs000325.w\n      (\n       OUTPUT       w-nam_bnk,\n       INPUT-OUTPUT w-num_bnk,\n       OUTPUT       v-cod_can\n      ).\n\n   IF v-cod_can = FALSE\n   THEN DO:\n      RUN Get_Frame_Rel_Fields_num_bnk.\n      RUN Dis_Frame_Rel_Fields_num_bnk.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Listbox_typ_acc C-Win\nPROCEDURE Start_Listbox_typ_acc :\n/*------------------------------------------------------------------------------\n  Purpose : Start Listbox op veld w-typ_acc\n------------------------------------------------------------------------------*/\n\n   ASSIGN INPUT FRAME {&FRAME-NAME} w-typ_acc.\n   RUN lbx_bnk_acc_typ.w\n     (\n      INPUT-OUTPUT v-bnk_acc_grp,\n      INPUT-OUTPUT w-typ_acc,\n      OUTPUT       w-dsc_acc,\n      OUTPUT       v-rowid,\n      OUTPUT       v-cod_can\n     ).\n\n   IF v-cod_can = FALSE\n   THEN DO:\n      RUN Get_Frame_Rel_Fields_typ_acc.\n      RUN Dis_Frame_Rel_Fields_typ_acc.\n   END.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Start_Program_Value C-Win\nPROCEDURE Start_Program_Value :\n/*------------------------------------------------------------------------------\n  Purpose : Start Program Value\n------------------------------------------------------------------------------*/\n\n   RUN Get_Procedure_Handle.\n   IF VALID-HANDLE(v-sub_prc_hdl) THEN DO:\n       v-wid_hdl = v-sub_prc_hdl:CURRENT-WINDOW.\n       IF VALID-HANDLE(v-wid_hdl) THEN DO:\n           v-flg     = v-wid_hdl:MOVE-TO-TOP() = TRUE.\n           ASSIGN v-wid_hdl:WINDOW-STATE = 3.\n       END.\n       RETURN.\n   END.\n   /***************************************************************************/\n   /* Start programma met bijbehorende parameters.                            */\n   /***************************************************************************/\n   CASE v-sub_prc_nam:\n   WHEN \"crs000324.w\":U  /* Bank */\n   THEN DO:\n      RUN VALUE(v-sub_prc_nam) PERSISTENT SET v-sub_prc_hdl\n         (\n          INPUT  w-num_bnk,\n          OUTPUT v-cod_can\n         ).\n   END.\n   WHEN \"crs000004.w\":U /* Currency */\n   THEN DO:\n      RUN VALUE(v-sub_prc_nam) PERSISTENT SET v-sub_prc_hdl\n         (\n          INPUT  w-cod_cur,\n          OUTPUT v-cod_can\n         ).\n   END.\n   END CASE.\n   /****************************************************************************/\n   /* Bijhouden procedure handles van opgestarte programma's                   */\n   /****************************************************************************/\n   CREATE sub_prc.\n   ASSIGN sub_prc.sub_prc_nam = v-sub_prc_nam\n          sub_prc.sub_prc_hdl = v-sub_prc_hdl\n          sub_prc.sub_prc_par = v-sub_prc_par.\n\n   /****************************************************************************/\n   /* Apply \"ENTRY\" TO CURRENT-WINDOW.                                         */\n   /****************************************************************************/\n   IF VALID-HANDLE(v-wid_hdl) THEN DO:\n       v-wid_hdl = v-sub_prc_hdl:CURRENT-WINDOW.\n       APPLY \"ENTRY\" TO v-wid_hdl.\n   END.\n\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Set_Bnk_Acc_Num_Fmt C-Win\nPROCEDURE Set_Bnk_Acc_Num_Fmt :\n/*------------------------------------------------------------------------------\n  Purpose: Set_Bnk_Acc_Num_Fmt\n------------------------------------------------------------------------------*/\nDEFINE VARIABLE v-IBAN_fmt AS CHARACTER NO-UNDO.\n\n   ASSIGN v-IBAN_fmt = {fna getIbanFormat w-num_bnk,\"v-sta_fun <> 'INQ':U\"}.\n\n   IF v-IBAN_fmt <> \"\":U\n   THEN\n      ASSIGN w-num_ctr_bnk_acc:FORMAT IN FRAME {&FRAME-NAME} = v-IBAN_fmt.\n   ELSE\n      ASSIGN w-num_ctr_bnk_acc:FORMAT IN FRAME {&FRAME-NAME} = \"X(50)\":U.\n\n   RETURN \"\":U.\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Update_Temp_Table_Buffers C-Win\nPROCEDURE Update_Temp_Table_Buffers :\n/*------------------------------------------------------------------------------\n  Purpose : Update Temp Table Buffers\n------------------------------------------------------------------------------*/\n\n   ASSIGN tmp_bnk_acc_ext_wrk.idt_bnk_acc      = w-idt_bnk_acc\n          tmp_bnk_acc_ext_wrk.typ_acc          = w-typ_acc\n          tmp_bnk_acc_ext_wrk.num_bnk          = w-num_bnk\n          tmp_bnk_acc_ext_wrk.cod_cur          = w-cod_cur\n          tmp_bnk_acc_ext_wrk.num_ctr_bnk_acc  = w-num_ctr_bnk_acc\n          tmp_bnk_acc_ext_wrk.nam_ctr_bnk_acc  = w-nam_ctr_bnk_acc\n          tmp_bnk_acc_ext_wrk.wpl_bnk_acc      = w-wpl_bnk_acc\n          tmp_bnk_acc_ext_wrk.sta_bnk_acc      = w-sta_bnk_acc\n          tmp_bnk_acc_ext_wrk.typ_src          = 'Manual':U\n          .\n\nEND PROCEDURE.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n/* ************************  Function Implementations ***************** */\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _FUNCTION getStatus C-Win\nFUNCTION getStatus RETURNS CHARACTER\n  ( /* parameter-definitions */ ) :\n/*------------------------------------------------------------------------------\n  Purpose : getStatus\n------------------------------------------------------------------------------*/\n\n  RETURN v-sta_fun.\n\nEND FUNCTION.\n\n/* _UIB-CODE-BLOCK-END */\n&ANALYZE-RESUME\n\n&ANALYZE-SUSPEND _UIB-CODE-BLOCK _FUNCTION setStatus C-Win\nFUNCTION setStatus RETURNS LOGICAL\n  ( p-sta AS CHARACTER ) :\n/*------------------------------------------------------------------------------\n  Purpose : setStatus, hierin worden alle status afhankelijke procedures doorgevoerd\n------------------------------------------------------------------------------*/\n\n   IF v-sta_fun = p-sta\n   THEN RETURN TRUE.\n\n   v-sta_fun = p-sta.\n\n   /*************************************************************************/\n   /* venster titel afhandeling                                             */\n   /*********************************************************